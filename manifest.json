{
  "name": "Chamada Meet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extensão feita com intuito de facilitar chamada na plataforma online Meet do colégio Bonja.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "css": [
        "css/index.css"
      ]
    }
  ]
}